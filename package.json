{
  "name": "onion-travel",
  "description": "Simple blog for OnionTravel",
  "author": "Sebastian Weso≈Çowski <sebastian@wesolowski.dev>",
  "version": "0.1.0",
  "scripts": {
    "start": "npm run develop",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "npm run build:dir && npm run build:favicons && gatsby build",
    "format": "prettier --config ./.prettierrc --write \"**/*.{js,jsx,json,md}\"",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build",
    "test": "ava **/*.test.js --verbose"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "core-js"
    ]
  },
  "private": true,
  "keywords": [
    "gatsby",
    "OnionTravel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianWesolowski/OnionTravel"
  },
  "bugs": {
    "url": "https://github.com/SebastianWesolowski/OnionTravel/issues"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.1",
    "@mdx-js/react": "^1.5.1",
    "babel-plugin-styled-components": "^1.10.6",
    "directory-named-webpack-plugin": "^4.0.1",
    "gatsby": "^2.15.34",
    "gatsby-image": "^2.2.25",
    "gatsby-plugin-mdx": "^1.0.52",
    "gatsby-plugin-offline": "^3.0.13",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-react-svg": "^2.1.2",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-sitemap": "^2.2.17",
    "gatsby-plugin-styled-components": "^3.1.9",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-transformer-json": "^2.2.13",
    "gatsby-transformer-remark": "^2.6.28",
    "gatsby-transformer-sharp": "^2.2.21",
    "intersection-observer": "^0.7.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-pose": "^4.0.9",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^3.4.1",
    "ava": "^0.25.0",
    "babel-eslint": "^10.0.3",
    "chrome-launcher": "^0.11.2",
    "core-js": "^3.2.1",
    "cross-env": "^7.0.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "favicons": "^5.4.1",
    "gatsby-plugin-eslint": "^2.0.5",
    "husky": "^4.0.2",
    "lighthouse": "^5.5.0",
    "lint-staged": "^10.0.3",
    "mkdirp": "^0.5.1",
    "node-gyp": "^6.0.0",
    "prettier": "^1.18.2",
    "stylelint": "^11.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "stylelint-webpack-plugin": "^1.0.2"
  },
  "unusableButInteresting": {
    "@reach/dialog": "^0.2.9",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.1.4",
    "gatsby-background-image": "^0.8.15",
    "gatsby-plugin-manifest": "^2.2.21",
    "react-slick": "^0.25.2"
  }
}
