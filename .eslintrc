{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "strict": 0,
    "no-console": "warn",
    "quotes": [
      "warn",
      "single"
    ],
    "prettier/prettier": "warn",
    "react/prop-types": "warn",
    "no-unused-vars": "warn",
    "react/jsx-filename-extension": 0,
    "padded-blocks": "warn"
    // "react/forbid-prop-types": "warn",
    // "react/no-array-index-key": 0,
  },
  "extends": [
    "airbnb",
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": ["prettier", "jsx-a11y"],

  "settings": {
    "react": {
      "version": "detect"
    }
    // "import/resolver": {
    //   "node": {
    //     "paths": ["./src"],
    //     "extensions": [".js"]
    //   }
    // }
  },
  "globals": {
    "window": true,
    "document": true
  }
}
